 "doctype": "Print Format",
 "font_size": 14,
 "html": "<style>\n{% set site_url = frappe.utils.get_url() %}\n\n@font-face {\n\tfont-family: 'Trebuchet';\n\tfont-style: normal;\n\tsrc: local('Trebuchet MS'), local('Trebuchet MS'), url('{{site_url}}/assets/jedermann/fonts/trebuc.ttf') format('truetype');\n}\n\n@font-face {\n\tfont-family: 'Trebuchet';\n\tfont-style: italic;\n\tsrc: local('Trebuchet MS'), local('Trebuchet MS'), url('{{site_url}}/assets/jedermann/fonts/Trebuchet-MS-Italic.ttf') format('truetype');\n}\n</style>\n\n{% set country = frappe.db.get_value(\"Address\", doc.customer_address, \"country\") if doc.customer_address else \"\" %}\n\n<div  id=\"header-html\">\n    <div class=\"visible-pdf text-right\" style=\"margin-top: -16mm;\">\n        {{ _(\"Seite:  {0}/{1}, \").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }} {{ doc.name }}\n    </div>\n    <div class=\"print-format-header\" style=\"margin-top: -1mm;\">\n        <img style=\"width: 33.5%\" src=\"{{site_url}}/assets/jedermann/img/Jedermann_Logo_mUt_NEU_resized.png\">\n    </div>\n</div>\n\n<div style=\"margin-top: 3.5mm; margin-left:0.1cm;\">\n    {% set currency_symbol = frappe.db.get_value(\"Currency\", doc.currency, \"symbol\") %}\n    \n    <table width=\"100%\">\n        <tr>\n        \t<td width=\"56%\">\n    \t\t\t<span style='letter-spacing: 0.2mm;'>Jedermann AG \u2022 Hauptstrasse 33 \u2022 D-56291 Hausbay</span><br>\n    \t\t</td>\n        \t<td width=\"44%\" style=\"text-align: left; font-weight: 800; font-size: 19px; line-height: 20px;\" >{{ _(\"Auftragsbest\u00e4tigung\") }}</td>\n        </tr>\n    </table>\n\n    <table width=\"100%\" style=\"margin-top: -4mm;\">\n    \t<tr>\n    \t\t<!-- Left Side Content -->\n    \t\t<td width=\"56%\">\n    \t\t\t<div style=\"margin-top: 14px;\">\n    \t\t\t    {{doc.customer_name }}<br>\n    \t\t\t    {{doc.address_display or \"\" }}<br>\n    \t\t\t</div>\n    \t\t</td>\n    \t\t<!-- Right Side Content -->\n    \t\t<td width=\"44%\">\n    \t\t\t\t<table style=\"width:90%; border-collapse: collapse;\" class=\"custom-item-table\">\n    \t\t\t\t\t<tr>\n    \t\t\t\t\t\t<td style=\"width:47%;\"></td>\n    \t\t\t\t\t\t<td style=\"width:53%;\"></td>\n    \t\t\t\t\t</tr>\n    \t\t\t\t\t<tr>\n    \t\t\t\t\t    <td colspan=\"2\"><br></td>\n    \t\t\t\t\t</tr>\n    \t\t\t\t\t<tr>\n    \t\t\t\t\t\t<td style=\"text-align: left;\">{{_(\"Auftrag-Nr.\")}}:</td>\n    \t\t\t\t\t\t<td style=\"text-align: left;\">{{doc.name or \"\"}}</td>\n    \t\t\t\t\t</tr>\n    \t\t\t\t\t<tr>\n    \t\t\t\t\t\t<td style=\"text-align: left;\">{{ _(\"Datum\") }}:</td>\n    \t\t\t\t\t\t<td style=\"text-align: left;\">{{ doc.get_formatted(\"transaction_date\") }}</td>\n    \t\t\t\t\t</tr>\n    \t\t\t\t\t<tr>\n    \t\t\t\t\t\t<td style=\"text-align: left;\">{{ _(\"Kunden-Nr.\") }}:</td>\n    \t\t\t\t\t\t<td style=\"text-align: left;\">{{ doc.customer }}</td>\n    \t\t\t\t\t</tr>\n    \t\t\t\t\t<tr>\n    \t\t\t\t\t\t<td style=\"text-align: left;\">{{ _(\"Kunden-Best.-Nr.\") }}:</td>\n    \t\t\t\t\t\t<td style=\"text-align: left;\">{{ doc.po_no or \"\" }}</td>\n    \t\t\t\t\t</tr>\n    \t\t\t\t</table>\n    \t\t</td>\n    \t</tr>\n    </table>\n</div>\n\n<div style=\"margin-top: 18mm;\">\n    {% set item_col_width = frappe.utils.cstr(get_article_and_description_column_width(doc.items, \"item_code\", 54)) + \"%\"%}\n    <table width=\"100%\" style=\"border-collapse: collapse;\">\n      <thead>\n        <tr style=\"border-top: 1px solid #333c44; border-bottom: 1px solid #333c44;\">\n            <td width=\"5%\" class=\"text-bold\" ><b>{{ _(\"Pos.\") }}</b></td>\n            <td width=\"{{get_article_and_description_column_width(doc.items, \"item_code\", 54)}}%\" class=\"text-bold\" style=\"text-align: left;\"><b>{{ _(\"Artikel\") }}</b></td>\n            <td width=\"{{get_article_and_description_column_width(doc.items, \"description\", 54)}}%\" class=\"text-bold\" style=\"text-align: left;\"><b>{{ _(\"Bezeichnung\") }}</b></td>\n            <td width=\"10%\" class=\"text-bold\" style=\"text-align: center;\"><b>{{ _(\"Menge\") }}</b></td>\n            <td width=\"2%\"></td>\n            <td width=\"14%\" class=\"text-bold\" style=\"text-align: center;\"><b>{{ _(\"Preis\") }}</b></td>\n            <td width=\"15%\" class=\"text-bold\" style=\"text-align: right\"><b>{{ _(\"Gesamt\") }}</b></td>\n        </tr>\n\n      </thead>\n      \n      <!-- Table Body -->\n      <tbody style=\"font-size:3.2mm;\">\n        \n        {%- for row in doc.items -%}\n            <tr style=\"border-bottom: 1px solid #333c44;\">\n                <td style=\"padding-top:1 !important; padding-bottom:0 !important;\">{{ row.idx }}</td>\n                <td style=\"padding-top:1 !important; padding-bottom:0 !important; text-align: left; word-wrap:break-word;\">\n                    <div style=\"width:84px\">{% if row.customer_item_code %}<b>{{ row.customer_item_code or \"\" }}</b><br>{% endif %} {{ row.item_code }}</div>\n                </td>\n                \n                <td style=\"padding-top:1 !important; padding-bottom:0 !important; text-align: left;\">{{ row.description }}</td>\n                <td style=\"padding-top:1 !important; padding-bottom:0 !important; text-align: center; word-wrap:break-word;\">\n                    <div style=\"width:57px\">{{ row.get_formatted(\"qty\") or \"\" }} {{ row.uom or \"\" }}</div>\n                </td>\n                <td></td>\n                <td style=\"padding-top:1 !important; padding-bottom:0 !important; text-align: center; word-wrap:break-word;\">\n                    <div style=\"width:45px; text-align: center;\">\n                        {{ frappe.utils.fmt_money(row.rate or '', precision=0) }}  {{ currency_symbol or \"\" }}\n                    </div>\n                </td>\n                <td style=\"padding-top:1 !important; padding-bottom:0 !important; text-align: right;\">\n                    {{ frappe.utils.fmt_money(row.amount or '', precision=0 ) }} {{ currency_symbol or \"\" }}\n                </td>\n            </tr>\n        {%- endfor -%}\n      </tbody>\n    </table> \n</div>\n\n<!-- Summary Section -->\n<div style=\"margin-top: 20px;\">\n    <table width=\"100%\" class=\"custom-item-table\" style=\"page-break-inside: avoid;\">\n        <tr>\n            <td width=\"50%\" colspan=\"2\"></td>\n            <td width=\"50%\" colspan=\"2\">\n                <div style=\"border-top: 1px solid #000;\"></div>\n            </td>\n        </tr>\n        <tr>\n            <td width=\"50%\" colspan=\"2\"></td>\n            <td width=\"25%\">{{ _(\"Summe-Netto:\") }}</td>\n            <td width=\"25%\" align=\"right\">{{ doc.get_formatted(\"total\") }}</td>\n        </tr>\n        <tr>\n            <td width=\"50%\" colspan=\"2\"></td>\n            <td width=\"25%\">{{ _(\"+ 19 % MwSt.:\") }}</td>\n            <td width=\"25%\" align=\"right\">{{ doc.get_formatted(\"total_taxes_and_charges\") }}</td>\n        </tr>\n        <tr>\n            <td width=\"50%\" colspan=\"2\"></td>\n            <td width=\"50%\" colspan=\"2\">\n                <div style=\"border-top: 1px solid #000;\"></div>\n            </td>\n        </tr>\n        <tr>\n            <td width=\"50%\" colspan=\"2\"></td>\n            <td width=\"25%\"><strong>{{ _(\"Summe-Brutto:\") }}</strong></td>\n            <td width=\"25%\" align=\"right\"><strong>{{ doc.get_formatted(\"grand_total\") }}</strong></td>\n        </tr>\n    \n        <tr>\n            <td width=\"50%\" colspan=\"2\"></td>\n            <td width=\"50%\" colspan=\"2\">\n                <div style=\"border-top: 1px solid #000;\"></div>\n            </td>\n        </tr>\n        <tr>\n            <td width=\"50%\" colspan=\"2\"></td>\n            <td width=\"50%\" colspan=\"2\">\n                <div style=\"border-top: 1px solid #000; margin-top: 3px;\"></div>\n            </td>\n        </tr>\n        {% set yy = frappe.utils.get_datetime(doc.delivery_date).strftime(\"%Y\")  %}\n\n        {% if doc.delivery_date and yy %}\n        <tr>\n            <td width=\"25%\" style=\"vertical-align: top;\">\n                <span style=\"margin-left: 5px;\">{{ _(\"Liefertermin\") }}:</span>\n            </td>\n            <td width=\"25%\" style=\"vertical-align: top;\">\n                <div style=\"margin-bottom: 5px;\">KW {{ doc.custom_week_number or \"\" }} {{ yy or \"\" }}</div>\n            </td>            \n            <td width=\"50%\" colspan=\"2\"></td>\n        </tr>\n        {% endif %}\n        {% if doc.incoterm %}\n        <tr>\n            <td width=\"25%\" style=\"vertical-align: top;\">\n                    <span style=\"margin-left: 5px;\">{{ _(\"Lieferbedingung\") }}:</span>\n            </td>\n            <td width=\"25%\" style=\"vertical-align: top; text-align: left;\">\n                <div style=\"margin-bottom: 5px;\">{{ doc.incoterm or \"\" }}-{{ doc.named_place or \"\" }}</div>\n            </td>            \n            <td width=\"50%\" colspan=\"2\"></td>\n        </tr>\n        {% endif %}\n        \n        {% if country and country != \"Germany\" %}\n            <tr>\n                <td width=\"25%\"><span style=\"margin-left: 5px;\">{{ _(\"UID Nummer\") }}:</span> </p></td>\n                <td width=\"25%\"><p>{{ doc.tax_id  or \"\" }} </p></td>\n                <td width=\"50%\" colspan=\"2\"></td>\n            </tr>\n        {% endif %}\n        \n        {% if doc.payment_schedule %}\n        <tr>\n            <td width=\"25%\" style=\"vertical-align: top;\">\n                <span style=\"margin-left: 5px;\">{{ _(\"Payment Terms\") }}:</span>\n            </td>\n            <td width=\"25%\" style=\"vertical-align: top; text-align: left;\">\n                {%- for row in doc.payment_schedule -%}\n                    <span style=\"margin-bottom: 5px; margin-left: 5px;\" >{{ row.description or \"\" }}</span>\n                {%- endfor -%}\n            </td>  \n            <td width=\"50%\" colspan=\"2\"></td>\n        </tr>\n        {% endif %}\n        {% if doc.terms %}\n        <tr>\n            <td width=\"50%\" colspan=\"2\" style=\"vertical-align: top;\">\n                <span style=\"margin-left: 5px;\">{{ _(doc.terms) }}</span>\n            </td>\n            <td width=\"50%\" colspan=\"2\"></td>\n        </tr>\n        {% endif %}\n    \n    </table>\n</div>\n\n<!-- Footer Section -->\n<div id=\"footer-html\" style=\"margin-top:5px;\">\n  <div class=\"print-footer\">\n    {% if footer %}\n      <div class=\"letter-head\">{{ footer }}</div>\n    {% endif %}\n  </div>\n</div>\n",
