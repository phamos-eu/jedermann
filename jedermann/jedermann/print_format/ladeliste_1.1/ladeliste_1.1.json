 "font_size": 14,
 "html": "<style>\n{% set site_url = frappe.utils.get_url() %}\n@font-face {\n\tfont-family: 'ArialGreek';\n\tfont-style: normal;\n\tsrc: local('Arial Greek'), local('Arial Greek'), url('{{site_url}}/assets/jedermann/fonts/arial-greek-regular.ttf') format('truetype');\n}\n</style>\n\n<div  id=\"header-html\">\n    <div style=\"margin-top: -8mm;\">\n        <table width=\"100%\" style=\"border: 2px solid black;\">\n            <tr >\n                <td style=\"border-right:2px solid black;\" rowspan=2 width=\"49%\" ><b>{{ _(\"Lade-Liste\") }}</b></td>\n                <td style=\"border-right:2px solid black;\" width=\"23%\" ></td>\n                <td style=\"border-right:2px solid black;\" width=\"28%\"></td>\n            </tr>\n            <tr>\n                <td style=\"border-right:2px solid black;\" width=\"23%\">{{ _(\"BM = Bestell - Menge\") }}</td>\n                <td style=\"border-right:2px solid black;\" width=\"28%\"></td>\n            </tr>\n            <tr>\n                <td style=\"border-right:2px solid black;\" rowspan=2 width=\"49%\">{{ _(\"Jedermann AG\") }}</td>\n                <td style=\"border-right:2px solid black;\" width=\"23%\">{{ _(\"GM = Gelieferte - Menge\") }}</td>\n                <td style=\"border-right:2px solid black;\" width=\"28%\">{{ _(\"Datum\") }}: {{ doc.get_formatted(\"modified\")}} Uhr</td>\n            </tr>\n            <tr>\n                <td style=\"border-right:2px solid black;\" width=\"23%\">{{ _(\"OM = Offene - Menge\") }}</td>\n                <td style=\"border-right:2px solid black;\" width=\"28%\">{{ _(\"Seite:  {0}/{1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}</td>\n            </tr>\n        </table>\n    </div>\n\n</div>\n\n<br>\n<table width=\"100%\" class=\"table-bordered\" stytle=\"padding-top:2mm;\">\n    <head>\n        <tr>\n            <td>{{ _(\"LS\") }}</td>\n            <td>{{ _(\"vst.\") }}</td>\n            <td>{{ _(\"Menge\") }}</td>\n            <td>{{ _(\"Palette\") }}</td>\n            <td>{{ _(\"Auftrag\") }}</td>\n            <td>{{ _(\"Intern\") }}</td>\n            <td>{{ _(\"Artikel-Nr.\") }}</td>\n            <td>{{ _(\"Charge\") }}</td>\n            <td>{{ _(\"BM\") }}</td>\n            <td>{{ _(\"GM\") }}</td>\n            <td>{{ _(\"OM\") }}</td>\n            <td>{{ _(\"Lief.Termin\") }}</td>\n        </tr>\n    </head>\n    <tbody>\n        {% for item in doc.items %}\n        <tr>\n            <td></td>\n            <td></td>\n            <td></td>\n            <td></td>\n            <td>{{ doc.po_no or \"\" }}</td>\n            <td>{{ doc.name }}</td>\n            <td>{{ item.item_code}}</td>\n            <td></td>\n            <td>{{ item.get_formatted(\"qty\") }}</td>\n            <td>{{ item.get_formatted(\"delivered_qty\") }}</td>\n            <td>{{ frappe.utils.cint(item.qty - item.delivered_qty) }}</td>\n            <td>{{ doc.get_formatted(\"delivery_date\") }}</td>\n        </tr>\n        {% endfor %}\n    </tbody>\n</table>",
