 "font_size": 14,
 "html": "<style>\r\n{% set site_url = frappe.utils.get_url() %}\r\n@font-face {\r\n\tfont-family: 'Trebuchet';\r\n\tfont-style: normal;\r\n\tsrc: local('Trebuchet MS'), local('Trebuchet MS'), url('{{site_url}}/assets/jedermann/fonts/trebuc.ttf') format('truetype');\r\n}\r\n\r\n@font-face {\r\n\tfont-family: 'Trebuchet';\r\n\tfont-style: italic;\r\n\tsrc: local('Trebuchet MS'), local('Trebuchet MS'), url('{{site_url}}/assets/jedermann/fonts/Trebuchet-MS-Italic.ttf') format('truetype');\r\n}\r\n</style>\r\n \r\n{% set country = frappe.db.get_value(\"Address\", doc.customer_address, \"country\") if doc.customer_address else \"\" %}\r\n\r\n<div  id=\"header-html\">\r\n    <div class=\"visible-pdf text-right\" style=\"margin-top: -16mm;\">\r\n        {{ _(\"Seite:  {0}/{1}, \").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }} {{ doc.name }}\r\n    </div>\r\n    <div class=\"print-format-header\" style=\"margin-top: -1mm;\">\r\n        <img style=\"width: 33.5%\" src=\"{{site_url}}/assets/jedermann/img/Jedermann_Logo_mUt_NEU_resized.png\">\r\n    </div>\r\n</div> \r\n  \r\n<div style=\"margin-top: 3.5mm; margin-left:0.1cm;\">\r\n    {% set currency_symbol = frappe.db.get_value(\"Currency\", doc.currency, \"symbol\") %}\r\n\r\n    <table width=\"100%\">\r\n        <tr>\r\n        \t<td width=\"56%\">\r\n    \t\t\t<span style='letter-spacing: 0.2mm;'>Jedermann AG \u2022 Hauptstrasse 33 \u2022 D-56291 Hausbay</span><br>\r\n    \t\t</td>\r\n\t\t\t<td width=\"44%\" style=\"text-align: left; font-weight: 800; font-size: 19px; line-height: 20px; \">{{ _(doc.custom_sales_invoice_type) or _(\"Bestellung\") }}</td>\r\n        </tr>\r\n    </table>\r\n\r\n    <table width=\"100%\" style=\"margin-top: -5mm;\">\r\n    \t<tr>\r\n    \t\t<!-- Left Side Content -->\r\n    \t\t<td width=\"56%\">\r\n    \t\t\t<div style=\"margin-top: 14px;\">\r\n    \t\t\t    {{doc.supplier_name }}<br>\r\n    \t\t\t    {{doc.address_display or \"\" }}<br>\r\n    \t\t\t</div>\r\n    \t\t</td>\r\n    \t\t<!-- Right Side Content -->\r\n    \t\t<td width=\"44%\">\r\n    \t\t\t\t<table style=\"width:100%; border-collapse: collapse;\" class=\"custom-item-table meta-information-table\">\r\n    \t\t\t\t\t<tr>\r\n    \t\t\t\t\t\t<td style=\"width:47%;\"></td>\r\n    \t\t\t\t\t\t<td style=\"width:53%;\"></td>\r\n    \t\t\t\t\t</tr>\r\n    \t\t\t\t\t<tr>\r\n    \t\t\t\t\t    <td colspan=\"2\"><br></td>\r\n    \t\t\t\t\t</tr>\r\n    \t\t\t\t\t<tr>\r\n    \t\t\t\t\t\t<td style=\"text-align: left;\">{{_(\"Rechnung-Nr.\")}}:</td>\r\n    \t\t\t\t\t\t<td style=\"text-align: left;\">{{doc.name}}</td>\r\n    \t\t\t\t\t</tr>\r\n    \t\t\t\t\t<tr>\r\n    \t\t\t\t\t\t<td style=\"text-align: left;\">{{ _(\"Datum\") }}:</td>\r\n    \t\t\t\t\t\t<td style=\"text-align: left;\">{{ doc.get_formatted(\"transaction_date\") }}</td>\r\n    \t\t\t\t\t</tr>\r\n    \t\t\t\t\t<tr>\r\n    \t\t\t\t\t\t<td style=\"text-align: left;\">{{ _(\"Lieferanten-Nr.\") }}:</td>\r\n    \t\t\t\t\t\t<td style=\"text-align: left;\">{{ doc.supplier }}</td>\r\n    \t\t\t\t\t</tr>\r\n                        {% if doc.custom_sales_invoice_type != \"Korrekturrechnung\" %}\r\n    \t\t\t\t\t\t<tr>\r\n    \t\t\t\t\t\t\t<td style=\"text-align: left;\">{{ _(\"Ihre Angebots-Nr.\") }}:</td>\r\n    \t\t\t\t\t\t\t<td style=\"text-align: left;\">{{ doc.custom_supplier_quote_reference or \"\" }}</td>\r\n    \t\t\t\t\t\t</tr>\r\n    \t\t\t\t\t\t<tr>\r\n    \t\t\t\t\t\t\t<td style=\"text-align: left;\">{{ _(\"Unsere Sachbearbeiter\") }}:</td>\r\n    \t\t\t\t\t\t\t<td style=\"text-align: left;\">{{ frappe.session.user }}</td>\r\n    \t\t\t\t\t\t</tr>\r\n                        {% endif %}\r\n\r\n    \t\t\t\t</table>\r\n    \t\t</td>\r\n    \t</tr>\r\n    </table>\r\n</div>\r\n\r\n<div style=\"margin-top: 18mm;\">\r\n    <table width=\"100%\" style=\"border-collapse: collapse;\" >\r\n      <thead>\r\n        <tr style=\"border-top: 1px solid #333c44; border-bottom: 1px solid #333c44;\">\r\n            <td width=\"5%\" class=\"\" ><b>{{ _(\"Pos.\") }}</b></td>\r\n            <td width=\"16%\" class=\"\" style=\"text-align: left;\"><b>{{ _(\"Artikel\") }}</b></td>\r\n            <td width=\"36%\" class=\"\" style=\"text-align: left;\"><b>{{ _(\"Bezeichnung\") }}</b></td>\r\n\r\n            <td width=\"14%\" class=\"\" style=\"text-align: center;\"><b>{{ _(\"Menge\") }}</b></td>\r\n            \r\n            <td width=\"13%\" class=\"\" style=\"text-align: center;\"><b>{{ _(\"Preis\") }}</b></td>\r\n            \r\n            <td width=\"16%\" class=\"\" style=\"text-align: right\"><b>{{ _(\"Gesamt\") }}</b></td>\r\n        </tr>\r\n\r\n      </thead>\r\n      \r\n      <!-- Table Body -->\r\n      <tbody style=\"font-size:3.2mm;\">\r\n        {%- for row in doc.items -%}\r\n            <tr style=\"border-bottom: 1px solid #333c44;\">\r\n                <td style=\"padding-bottom:0 !important;\">{{ row.idx }}</td>\r\n                <td style=\"padding-bottom:0 !important; text-align: left;\">\r\n                    {% if row.supplier_part_no %}<strong>{{ row.supplier_part_no or \"\" }}</strong><br>{% endif %} {{ row.item_code }}\r\n                </td>\r\n                <td style=\"padding-bottom:0 !important; text-align: left;\">\r\n                    <span>{{ row.item_name }}</span><br>\r\n                </td>\r\n              \r\n                <td style=\"padding-bottom:0 !important; text-align: center; word-wrap:break-word;\">\r\n               \r\n                        {{ row.get_formatted(\"qty\") or \"\" }} {{ row.uom or row.stock_uom or \"\" }}\r\n                \r\n                </td>\r\n     \r\n                <td style=\"padding-bottom:0 !important; text-align: center; word-wrap:break-word;\">\r\n               \r\n                        {{ frappe.utils.fmt_money(row.rate or '') }} {{ currency_symbol }}\r\n                 \r\n                </td>\r\n             \r\n                <td style=\"padding-bottom:0 !important; text-align: right; word-wrap:break-word;\">\r\n                   \r\n                        {{ frappe.utils.fmt_money(row.amount or '') }} {{ currency_symbol }}\r\n                 \r\n                </td>\r\n            </tr>\r\n        {%- endfor -%}\r\n      </tbody>\r\n    </table> \r\n</div>\r\n\r\n<!-- Summary Section -->\r\n<div style=\"margin-top: 20px;\">\r\n    <table width=\"100%\" class=\"custom-item-table\" style=\"page-break-inside: avoid;\">\r\n        <tr>\r\n            <td width=\"50%\" colspan=\"2\"></td>\r\n            <td width=\"50%\" colspan=\"2\">\r\n                <div style=\"border-top: 1px solid #000;\"></div>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td width=\"50%\" colspan=\"2\"></td>\r\n            <td width=\"25%\">{{ _(\"Summe-Netto:\") }}</td>\r\n            <td width=\"25%\" align=\"right\">{{ frappe.utils.fmt_money(doc.total or '') }} {{ currency_symbol }}</td>\r\n        </tr>\r\n\r\n        <tr>\r\n            <td width=\"50%\" colspan=\"2\"></td>\r\n            <td width=\"25%\">{{ _(\"+ 19 % MwSt.:\") }}</td>\r\n            <td width=\"25%\" align=\"right\">{{ frappe.utils.fmt_money(doc.total_taxes_and_charges or '') }} {{ currency_symbol }}</td>\r\n        </tr>\r\n        <tr>\r\n            <td width=\"50%\" colspan=\"2\"></td>\r\n            <td width=\"50%\" colspan=\"2\">\r\n                <div style=\"border-top: 1px solid #000;\"></div>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td width=\"50%\" colspan=\"2\"></td>\r\n            <td width=\"25%\"><strong>{{ _(\"Summe-Brutto:\") }}</strong></td>\r\n            <td width=\"25%\" align=\"right\"><strong>{{ frappe.utils.fmt_money(doc.grand_total or '') }} {{ currency_symbol }}</strong></td>\r\n        </tr>\r\n        <tr>\r\n            <td width=\"50%\" colspan=\"2\"></td>\r\n            <td width=\"50%\" colspan=\"2\">\r\n                <div style=\"border-top: 1px solid #000;\"></div>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td width=\"50%\" colspan=\"2\"></td>\r\n            <td width=\"50%\" colspan=\"2\">\r\n                <div style=\"border-top: 1px solid #000; margin-top: 3px;\"></div>\r\n            </td>\r\n        </tr>\r\n        \r\n        {% if country and country != \"Germany\" %}\r\n            <tr>\r\n                <td width=\"25%\"><span style=\"margin-left: 5px;\">{{ _(\"UID Nummer\") }}:</span> </p></td>\r\n                <td width=\"25%\"><p>{{ doc.tax_id  or \"\" }} </p></td>\r\n                <td width=\"50%\" colspan=\"2\"></td>\r\n            </tr>\r\n        {% endif %}\r\n        \r\n        <tr>\r\n            <td width=\"50%\" colspan=\"2\">&nbsp;</td>\r\n            <td width=\"50%\" colspan=\"2\">&nbsp;</td>\r\n        </tr>\r\n        \r\n        <tr>\r\n            <td width=\"20%\" style=\"vertical-align: top;\">\r\n                <span style=\"margin-left: 5px;\">{{ _(\"Lieferbedingungen\") }}:</span>\r\n            </td>\r\n            \r\n             <td width=\"75%\" colspan=\"3\" style=\"vertical-align: top; text-align: left;\">\r\n               {{ doc.incoterm or \"\" }} - {{ doc.named_place or \"\" }}\r\n             </td>\r\n        </tr>\r\n        \r\n        <tr>\r\n            <td width=\"50%\" colspan=\"2\">&nbsp;</td>\r\n            <td width=\"50%\" colspan=\"2\">&nbsp;</td>\r\n        </tr>\r\n        \r\n        <tr>\r\n            <td width=\"25%\" style=\"vertical-align: top;\">\r\n                <span style=\"margin-left: 5px;\">{{ _(\"Zahlungsbedingungen\") }}:</span>\r\n            </td>\r\n           \r\n            <td width=\"75%\" colspan=\"3\" style=\"vertical-align: top; text-align: left;\">\r\n                {%- for row in doc.payment_schedule -%}\r\n                    <div style=\"margin-bottom: 5px;\">{{ row.description or \"\" }}</div>\r\n                {%- endfor -%}\r\n            </td>\r\n        </tr>\r\n        \r\n        {% if doc.terms %}\r\n        <tr>\r\n            <td width=\"100%\" colspan=\"3\" style=\"vertical-align: top;\">\r\n                <span>{{ _(doc.terms) }}</span>\r\n            </td>\r\n        </tr>\r\n        {% endif %}\r\n    </table>\r\n</div>\r\n\r\n<!-- Footer Section -->\r\n<div id=\"footer-html\" style=\"margin-top:5px;\">\r\n  <div class=\"print-footer\">\r\n    {% if footer %}\r\n      <div class=\"letter-head\">{{ footer }}</div>\r\n    {% endif %}\r\n  </div>\r\n</div>\r\n",
