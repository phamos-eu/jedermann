 "css": ".print-format-header {\n    top: 0mm; \n    text-align: center;\n    padding-right: 25px !important;\n}\n\n.ql-editor {\n    overflow: hidden !important;\n}\n\n.print-format th{\n    color: #000000 !important;\n    font-weight: bold !important;\n    padding: 0px !important;\n}\n\n.print-format td.text-bold, .print-format th.text-bold {\n    padding: 0px !important;\n}\n\ndiv.print-format > div.wrapper > div.print-format-header {\n    margin-top: -17mm;\n}\n\ndiv.print-format > div.wrapper > div.print-footer {\n\tmargin-bottom: -2mm;\n\tmargin-left: -2mm;\n}\n\n.print-format-gutter {\n\tpadding-top: 1mm !important;\n}\n\n.print-format {\n\tfont-family: 'Trebuchet';\n\tfont-size: 3.5mm;\n    padding-left: 21mm;\n\tpadding-right: 15mm;\n\tmargin-left: 0mm;\n\tmargin-right: 0mm;\n}\n\n.text-bold {\n    font-weight: 600; \n    font-size: 1.1em;\n}\n\n.t-header-styling {\n    padding-top:1px !important;\n    padding-bottom:1px !important;\n}\n\n.meta-information-table td:nth-of-type(1) {\n    width: 54%;\n}\n\n.meta-information-table td:nth-of-type(2) {\n    width: 46%;\n}\n\n.print-format td, .print-format th {\n    padding: 1% !important;\n}\n\n.custom-item-table td {\n    padding: 0px !important;\n}",
