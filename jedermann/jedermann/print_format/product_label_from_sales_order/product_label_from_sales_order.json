 "html": "<style>\n\n{% set site_url = frappe.utils.get_url() %}\n\n@font-face {\n\tfont-family: 'Trebuchet';\n\tfont-style: normal;\n\tsrc: local('Trebuchet MS'), local('Trebuchet MS'), url('{{site_url}}/assets/jedermann/fonts/trebuc.ttf') format('truetype');\n}\n\n@font-face {\n\tfont-family: 'Trebuchet';\n\tfont-style: italic;\n\tsrc: local('Trebuchet MS'), local('Trebuchet MS'), url('{{site_url}}/assets/jedermann/fonts/Trebuchet-MS-Italic.ttf') format('truetype');\n}\n\n</style>\n\n<div style=\"font-size: 19px\">\n\n    <table style=\"text-align: center;\">\n        {% for i in range(0, doc.items|length, 2) %}\n        <tr>\n            <td style=\"text-align: center;\">\n                {{ doc.items[i]['qty'] }} {{ doc.items[i]['uom'] }}<br>\n                {{ doc.items[i]['description'] }}<br>\n                <b>{{ doc.items[i]['item_code'] }}</b><br>\n                Datum: {{ doc.get_formatted(\"transaction_date\") }} A-Nr.: {{ doc.name }}<br>\n                {{ doc.get_formatted(\"delivery_date\") }} P-Nr.:\n            </td>\n            {% if i+1 < doc.items|length %}\n            <td style=\"text-align: center;\">\n                {{ doc.items[i+1]['qty'] }} {{ doc.items[i]['uom'] }}<br>\n                {{ doc.items[i+1]['description'] }}<br>\n                <b>{{ doc.items[i+1]['item_code'] }}</b><br>\n                Datum: {{ doc.get_formatted(\"transaction_date\") }} A-Nr.: {{ doc.name }}<br>\n                {{ doc.get_formatted(\"delivery_date\") }} P-Nr.:\n            </td>\n            {% else %}\n            <td></td>\n            {% endif %}\n        </tr>\n        {% endfor %}\n    </table>\n\n</div>",
